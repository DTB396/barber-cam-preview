    transition: all 0.2s;
  }
  
  .view-case-btn:hover {
    background: #c49564;
    transform: translateX(4px);
  }
  
  .no-results {
    text-align: center;
    padding: 3rem 1rem;
    color: var(--color-muted, #a8a39e);
  }
  
  @media (max-width: 768px) {
    .cases-hero {
      padding: 2rem 1rem;
